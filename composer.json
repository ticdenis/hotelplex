{
    "version": "0.0.4",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.3",
        "lambdish/phunctional": "^1.0",
        "mark-gerarts/auto-mapper-plus": "^1.3",
        "overblog/graphql-bundle": "^0.11.11",
        "ramsey/uuid": "^3.8",
        "rbdwllr/reallysimplejwt": "^2.0",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "4.2.*",
        "webmozart/assert": "^1.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/AppBundle",
            "HotelPlex\\": "src/HotelPlex"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/AppBundle",
            "HotelPlex\\Tests\\": "tests/HotelPlex"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "overblog/graphiql-bundle": "^0.1.2",
        "symfony/phpunit-bridge": "4.2.*",
        "symfony/test-pack": "^1.0",
        "symfony/twig-bundle": "4.2.*",
        "symfony/var-dumper": "4.2.*",
        "symfony/web-server-bundle": "4.2.*"
    }
}
